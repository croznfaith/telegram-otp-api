{
  "version": 2,
  "builds": [
    { "src": "api/**/*.php", "use": "@vercel/php" },
    { "src": "bot.js", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ]
}
